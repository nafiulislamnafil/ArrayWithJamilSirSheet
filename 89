#include <stdio.h>
int main() {
int t;
scanf("%d", &t);
for(int i = 1; i <=t; i++) {
    int n;
    scanf("%d", &n);
    int a[n];
      for(int i = 0; i < n; i++) {
          scanf("%d", &a[i]);
      }
      if(n%2!=0){
            for(int i = (n/2); i <n; i++) {
            printf("%d ", a[i]);
            }
       }
      else if(n%2==0){
            for(int i = (n/2); i <n; i++) {
            printf("%d ", a[i]);
            }
       }
 printf("\n");
}
return 0;
}
