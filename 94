#include<stdio.h>
int main() {
int n,x,f=1;
scanf("%d", &n);
int a[n];
 for(int i = 0; i < n; i++) {
          scanf("%d", &a[i]);
}
scanf("%d", &x);
for(int i = 0;i <n; i++) {
            if(a[i]==x){
                printf("%d",i);
                f=0;
                break;
            }
}
if (f==1) printf("-1");
return 0;
}
